{
    "name": "@microfocus/ias-icons",
    "version": "1.0.10",
    "description": "Icon font for Micro Focus Identity, Access, and Security products",
    "author": "James Albright <james.albright>@microfocus.com> (https://github.com/jalbr74)",
    "license": "MIT",
    "bugs": "https://github.com/microfocus/ias-icons/issues",
    "homepage": "https://github.com/microfocus/ias-icons#readme",
    "private": true,
    "scripts": {
        "ng": "ng",
        "clean": "rimraf dist",
        "start": "ng serve --serve-path ias-icons",
        "build": "npm-run-all build:icons build:docs",
        "build:icons": "node src/font-generator",
        "build:docs": "ng build --configuration=production",
        "publish": "npm-run-all publish:docs publish:icons",
        "publish:docs": "push-dir --dir=dist/docs --branch=gh-pages --verbose --cleanup",
        "publish:icons": "npm publish dist/generated-font --access public",
        "increment-version": "npm version patch -m \"Added new icons\"",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e"
    },
    "dependencies": {
        "@angular/animations": "~10.1.2",
        "@angular/cdk": "^10.2.2",
        "@angular/common": "~10.1.2",
        "@angular/compiler": "~10.1.2",
        "@angular/core": "~10.1.2",
        "@angular/forms": "~10.1.2",
        "@angular/platform-browser": "~10.1.2",
        "@angular/platform-browser-dynamic": "~10.1.2",
        "@angular/router": "~10.1.2",
        "@ux-aspects/ux-aspects": "^2.1.4",
        "rxjs": "~6.6.0",
        "tslib": "^2.0.0",
        "zone.js": "~0.10.2"
    },
    "devDependencies": {
        "@angular-devkit/build-angular": "~0.1001.2",
        "@angular/cli": "~10.1.2",
        "@angular/compiler-cli": "~10.1.2",
        "@types/jasmine": "~3.5.0",
        "@types/jasminewd2": "~2.0.3",
        "@types/node": "^12.11.1",
        "codelyzer": "^6.0.0",
        "jasmine-core": "~3.6.0",
        "jasmine-spec-reporter": "~5.0.0",
        "karma": "~5.0.0",
        "karma-chrome-launcher": "~3.1.0",
        "karma-coverage-istanbul-reporter": "~3.0.2",
        "karma-jasmine": "~4.0.0",
        "karma-jasmine-html-reporter": "^1.5.0",
        "npm-run-all": "^4.1.5",
        "protractor": "~7.0.0",
        "push-dir": "^0.4.1",
        "rimraf": "^3.0.2",
        "ts-node": "~8.3.0",
        "tslint": "~6.1.0",
        "typescript": "~4.0.2",
        "webfonts-generator": "^0.4.0"
    }
}
